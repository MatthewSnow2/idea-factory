{
  "name": "idea-factory",
  "version": "0.1.0",
  "description": "Agentic Idea Factory - Continuous innovation pipeline",
  "main": "server/dist/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "tsx watch server/index.ts",
    "client:dev": "vite",
    "build": "npm run build:server && npm run build:client",
    "build:server": "tsc server/index.ts --outDir server/dist --module commonjs --target es2020",
    "build:client": "vite build",
    "preview": "vite preview",
    "db:init": "tsx server/db.ts",
    "db:reset": "rm -f data/ideas.db && tsx server/db.ts"
  },
  "dependencies": {
    "express": "^4.18.2",
    "sqlite3": "^5.1.6",
    "sqlite": "^5.0.1",
    "dotenv": "^16.3.1",
    "@anthropic-sdk/sdk": "^0.12.0",
    "@google/generative-ai": "^0.3.1",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/uuid": "^9.0.7",
    "typescript": "^5.3.3",
    "tsx": "^4.7.0",
    "concurrently": "^8.2.2",
    "vite": "^5.0.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "tailwindcss": "^3.4.1",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32"
  }
}
